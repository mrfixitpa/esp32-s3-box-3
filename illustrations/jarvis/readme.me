  thinking_illustration_file: https://github.com/mrfixitpa/esp32-s3-box-3/raw/main/illustrations/jarvis/thinking.png
  replying_illustration_file: https://github.com/mrfixitpa/esp32-s3-box-3/raw/main/illustrations/jarvis/replying.png
